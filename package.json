{
  "info": {
    "name": "awpaas-manager",
    "version": "1.0.0",
    "author": "afterloe[lm6289511@gmai.com]",
    "homePage": "https://github.com/afterloe",
    "remarks": "统一管理子系统：负责对平台进行统一管理的服务"
  },
  "server": {
    "addr": "127.0.0.1",
    "port": "8080",
    "multiCore": {
      "enable": false,
      "num": 4
    }
  },
  "dependencies": {
    "github.com/gin-gonic/gin": "laster",
    "github.com/docker/docker": "laster",
    "golang.org/x/net/context": "laster",
    "github.com/lib/pq": "laster",
    "github.com/satori/go.uuid": "laster",
    "github.com/mattn/go-sqlite3": "laster"
  },
  "services": {
    "docker-cli": {
      "version": "1.39",
      "socket": {
        "enable": true,
        "addr": "http://127.0.0.1:11000"
      }
    }
  }
}
